<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bgcolor" >
    <include layout="@layout/core_title" android:id="@+id/image_title"/>
   
    <TextView android:id="@+id/imagebrowse_test"
        android:layout_width="wrap_content" android:layout_height="wrap_content"
        android:text="2/2" android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"/>
    <android.support.v4.view.ViewPager android:id="@+id/imagebrowse_image"
        android:layout_width="wrap_content" android:layout_height="wrap_content"
        android:layout_above="@id/imagebrowse_test" android:layout_below="@id/image_title"/>
     
    

</RelativeLayout>
